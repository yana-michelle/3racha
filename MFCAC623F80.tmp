ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
    
}
li {
    float: left;
    clear: left;
    color: lightpink;
    text-align: left;
}

li a {
    display: block;
    color: gray;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
}

li a:hover{
    background-color: darkred;
}
a {
    margin: auto;
    padding: 10px;
    color: darkolivegreen;
	text-align: left;
    font-family: monospace;
    font-size: 90%;
}


footer {
	margin: auto;
