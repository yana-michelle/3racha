html{
     font-size: 10px;
  margin: 0;
  font-family: sans-serif;
}
@font-face {
    font-family: 'abeezeeregular';
    src: url('abeezee-regular-webfont.woff2') format('woff2'),
         url('abeezee-regular-webfont.woff') format('woff');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'intro_head_rbase';
    src: url('introheadr-base-webfont.woff2') format('woff2'),
         url('introheadr-base-webfont.woff') format('woff');
    font-weight: normal;
    font-style: normal;

}

body {
    background-color: black;
}

.container {
    
    border: 1px solid white;
    text-align: center;
}

.boxBoxGrey {
    border: 1px solid dimgray;
    background-color: dimgray;
}
.nav {
    text-decoration: underline;
}

title{
    font-family: 'Trade Winds', cursive;
}
h1 {
    padding: inherit;
    margin: 50px;
    bottom: 10px;
    text-align: center;
    color: red;
    font-family:"introheadr base";
    font-size: 200%;
}

h2 {
    margin: auto;
    padding: inherit;
    text-decoration: overline;
    bottom: 15px;
    left: 45px;
    right: 45px;
    top: 15px;
    text-align: left;
    color: darkred;
    font-family:'abeezeeregular';
    font-size: 200%;
}

h3 {
    text-align: center;
    color: antiquewhite;
